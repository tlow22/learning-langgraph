# LangSmith Monitoring 
LANGSMITH_TRACING="true"
LANGSMITH_API_KEY="..."
LANGSMITH_PROJECT="default"

# =============================================================================
# LangChain API Keys Template
# =============================================================================
# Copy this file to .env and fill in your actual API keys
# Remove or comment out any services you don't plan to use

# =============================================================================
# CORE LLM PROVIDERS
# =============================================================================

# OpenAI (Most Common)
OPENAI_API_KEY=sk-your-openai-api-key-here

# Anthropic (Claude)
ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key-here

# Google (Gemini)
GOOGLE_API_KEY=your-google-api-key-here

# Azure OpenAI
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
AZURE_OPENAI_API_KEY=your-azure-openai-api-key-here
AZURE_OPENAI_API_VERSION=2024-02-01

# =============================================================================
# POPULAR LLM PROVIDERS
# =============================================================================

# Groq
GROQ_API_KEY=gsk-your-groq-api-key-here

# Mistral AI
MISTRAL_API_KEY=your-mistral-api-key-here

# Cohere
COHERE_API_KEY=your-cohere-api-key-here

# Perplexity
PERPLEXITY_API_KEY=pplx-your-perplexity-api-key-here

# Together AI
TOGETHER_API_KEY=your-together-api-key-here

# =============================================================================
# VECTOR STORES & EMBEDDINGS
# =============================================================================

# Pinecone
PINECONE_API_KEY=your-pinecone-api-key-here
PINECONE_ENVIRONMENT=your-pinecone-environment

# Weaviate
WEAVIATE_API_KEY=your-weaviate-api-key-here
WEAVIATE_URL=https://your-cluster.weaviate.network

# Chroma (Local - no API key needed)
# CHROMA_PERSIST_DIRECTORY=./chroma_db

# Qdrant
QDRANT_API_KEY=your-qdrant-api-key-here
QDRANT_URL=https://your-qdrant-cluster.qdrant.io

# Vectara
VECTARA_API_KEY=your-vectara-api-key-here
VECTARA_CORPUS_ID=your-corpus-id

# =============================================================================
# TOOLS & INTEGRATIONS
# =============================================================================

# SerpAPI (Web Search)
SERPAPI_API_KEY=your-serpapi-key-here

# Zapier
ZAPIER_NLA_API_KEY=your-zapier-nla-api-key-here

# Wolfram Alpha
WOLFRAM_ALPHA_APPID=your-wolfram-alpha-app-id

# Weather API
OPENWEATHERMAP_API_KEY=your-openweathermap-api-key

# =============================================================================
# DATABASES & STORAGE
# =============================================================================

# PostgreSQL
POSTGRES_CONNECTION_STRING=postgresql://user:password@localhost:5432/dbname

# MongoDB
MONGODB_CONNECTION_STRING=mongodb://localhost:27017/

# Redis
REDIS_URL=redis://localhost:6379

# =============================================================================
# MONITORING & OBSERVABILITY
# =============================================================================

# LangSmith (LangChain's observability platform)
LANGSMITH_API_KEY=your-langsmith-api-key-here
LANGSMITH_TRACING=true
LANGSMITH_PROJECT=your-project-name

# =============================================================================
# SPECIALIZED SERVICES
# =============================================================================

# ElevenLabs (Text-to-Speech)
ELEVENLABS_API_KEY=your-elevenlabs-api-key-here

# AssemblyAI (Speech-to-Text)
ASSEMBLYAI_API_KEY=your-assemblyai-api-key-here

# Tavily (Web Search)
TAVILY_API_KEY=your-tavily-api-key-here

# =============================================================================
# ASIAN LLM PROVIDERS
# =============================================================================

# Baidu Qianfan
QIANFAN_AK=your-qianfan-access-key
QIANFAN_SK=your-qianfan-secret-key

# Zhipu AI
ZHIPUAI_API_KEY=your-zhipuai-api-key-here

# DashScope (Alibaba)
DASHSCOPE_API_KEY=your-dashscope-api-key-here

# =============================================================================
# ENTERPRISE & SPECIALIZED
# =============================================================================

# NVIDIA AI Foundation
NVIDIA_API_KEY=nvapi-your-nvidia-api-key-here

# SambaNova
SAMBANOVA_API_KEY=your-sambanova-api-key-here

# Baseten
BASETEN_API_KEY=your-baseten-api-key-here

# Banana.dev
BANANA_API_KEY=your-banana-api-key-here

# =============================================================================
# OPTIONAL: DEVELOPMENT & TESTING
# =============================================================================

# For local development with Ollama (no API key needed)
# OLLAMA_BASE_URL=http://localhost:11434

# For testing with mock responses
# MOCK_API_RESPONSES=true

# =============================================================================
# SECURITY NOTES
# =============================================================================
# 1. Never commit your actual .env file to version control
# 2. Use different API keys for development and production
# 3. Regularly rotate your API keys
# 4. Consider using a secrets manager for production environments